{
    "$schema": "http://json-schema.org/schema",
    "id": "generator-2",
    "type": "object",
    "title": "Generator 2",
    "description": "Description for generator 2",
    "examples": [
        "generator-2 --with-default . --with-named name"
    ],
    "mainDependencies": {
        "typescript": "*"
    },
    "devDependencies": {
        "tslint": "5.5.5",
        "jest": "^"
    },
    "properties": {
        "with-default": {
            "type": "string",
            "description": "With default.",
            "default": "."
        },
        "with-named": {
            "description": "With named",
            "type": "string",
            "x-prompt": "Please set named"
        }
    },
    "required": [
        "with-named"
    ]
}